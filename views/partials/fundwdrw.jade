.fundwdwr.partial.hide
  .col-md-6.col-md-offset-3.row
    .well
      form(class="form form-horizontal" id="fund-trf-form")
        h3 Request Withdrawal
        br
        .form-group
          label(class="control-label col-lg-2") Amount
          .col-lg-10
            input(class="form-control" ng-model="trfAmount" type="text")
        .form-group
          .col-lg-10.col-lg-offset-2
              a(href="" ng-click="reqWithdrawFund()" class="btn btn-primary") Withdraw
        br
        br
    .col-md-6.col-md-offset-3.row
        h3 Withdrawal History
        br
        a(href="" ng-click="getWithdrawalHist()" class="btn btn-primary") Get History
        include ./withhist