.creatagent.partial.hide
  .col-md-6.col-md-offset-3
    .well
      form(class="form form-horizontal" id="fund-trf-form")
        h3 Agent Registration Form
        br
        .form-group
          label(class="control-label col-lg-3") Name *
          .col-lg-8
            input(class="form-control" ng-model="agentName" type="text")
        .form-group
          label(class="control-label col-lg-3") Mobile Number *
          .col-lg-8
            input(class="form-control" ng-model="agentMob" type="text")
        .form-group
          label(class="control-label col-lg-3") Password *
          .col-lg-8
            input(class="form-control" ng-model="agentPwd" type="text")
        .form-group
          label(class="control-label col-lg-3") Commission *
          .col-lg-8
            input(class="form-control" ng-model="agentComm" type="text")
        .form-group
          label(class="control-label col-lg-3") State
          .col-lg-8
            input(class="form-control" ng-model="agentState" type="text")
        .form-group
          label(class="control-label col-lg-3") District
          .col-lg-8
            input(class="form-control" ng-model="agentDist" type="text")
        .form-group
          label(class="control-label col-lg-3") Pin
          .col-lg-8
            input(class="form-control" ng-model="agentPin" type="text")
        .form-group
          label(class="control-label col-lg-3") Address
          .col-lg-8
            input(class="form-control" ng-model="agentAdd" type="text")
        .form-group
          .col-lg-10.col-lg-offset-2
            a(href="" ng-click="createAgent()" class="btn btn-primary") Create
            a(href="" ng-click="clearAgentForm()" class="btn btn-primary") Clear